            <string>/// @desc Sonar - spacebar

var max_sonar_delay = 50;
var max_sonar_cooldown = 300; // 10 seconds-ish
sonar_active = false;

// Sending a state out to the sound director
if (sonar_active == true) {
    sonar_active = false;
}

if (keyboard_check_pressed(vk_space) and sonar_cooldown &lt; 0) {
    sonar_delay = max_sonar_delay;
    sonar_cooldown = max_sonar_cooldown;
    sonar_active = true;
    //TODO: FIx sonar
